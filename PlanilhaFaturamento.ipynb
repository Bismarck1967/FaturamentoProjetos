    "def CadastroQA(IdProj):\n",
    "    SqlProj = \"SELECT \\\n",
    "                  count(id_projeto) \\\n",
    "               FROM \\\n",
    "                  governanca_cgti_mapa.s_projeto_planejamento \\\n",
    "               where \\\n",
    "                  id_projeto = \" + str(IdProj)\n",
    "\n",
    "    cursorORC.execute(SqlProj)\n",
    "    \n",
    "    row = cursorORC.fetchall()   \n",
    "    \n",
    "    return(row)"
   ]
