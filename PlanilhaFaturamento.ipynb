    "def BuscaOcorrencia(IdProj):\n",
    "    SqlProj = \"SELECT \\\n",
    "                    O.id_projeto, \\\n",
    "                    O.ds_ocorrencia, \\\n",
    "                    to_char(O.dt_inicio,'DD/MM/RRRR') \\\n",
    "                FROM \\\n",
    "                    governanca_cgti_mapa.s_projeto_ocorrencia O \\\n",
    "                where \\\n",
    "                    o.id_tipo_ocorrencia <> 2 \\\n",
    "                and \\\n",
    "                    dt_inicio = (SELECT \\\n",
    "                                max(dt_inicio) \\\n",
    "                            FROM \\\n",
    "                                governanca_cgti_mapa.s_projeto_ocorrencia O1 \\\n",
    "                            where \\\n",
    "                                O1.id_projeto = O.id_projeto) \\\n",
    "                            and \\\n",
    "                                O.id_projeto = \" + str(IdProj)\n",
    "\n",
    "    cursorORC.execute(SqlProj)\n",
    "    \n",
    "    row = cursorORC.fetchall()   \n",
    "    \n",
    "    return(row)"
   ]
