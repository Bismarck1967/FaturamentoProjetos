    "def ConfCell(Linha):\n",
    "    \n",
    "    for Col in range(34):\n",
    "        pasta.cell(row = Linha, column = Col + 1).font = big_red_text\n",
    "        pasta.cell(row = Linha, column = Col + 1).alignment = center_aligned_text\n",
    "        pasta.cell(row = Linha, column = Col + 1).border  = square_border\n",
    "        pasta.cell(row = Linha, column = Col + 1).fill = cinza_background\n",
    "        \n",
    "    pasta.cell(row = Linha, column = 9).fill = Digita_background \n",
    "    pasta.cell(row = Linha, column = 10).fill = Digita_background \n",
    "    pasta.cell(row = Linha, column = 11).fill = Digita_background\n",
    "    pasta.cell(row = Linha, column = 12).fill = Digita_background\n",
    "    pasta.cell(row = Linha, column = 33).fill = Digita_background \n",
    "    return\n",
    "\n",
    "def ConfGrade(Linha):\n",
    "    for Col in range(34):\n",
    "        pasta.cell(row = Linha, column = Col + 1).border  = square_border\n",
    "    return\n",
    "\n",
    "def QuebraLinha(Linha):\n",
    "    default_height = 12\n",
    "    multiples_of_font_size2 = [default_height]\n",
    "    wrap_text = True\n",
    "    vertical = \"center\"\n",
    "\n",
    "    Div = [len(str(pasta.cell(row = Linha, column = i + 1).value)) if pasta.cell(row = Linha, column = i + 1).value != None else 0 for i in range(34)]\n",
    "\n",
    "    if max(Div) > 60:\n",
    "        Div = 30\n",
    "    else:\n",
    "        Div = 12\n",
    "\n",
    "    for Col in range(34):\n",
    "        mul = 0\n",
    "        for v in str(pasta.cell(row = Linha, column = Col + 1).value).split('\\n'):\n",
    "            mul += math.ceil(len(v) / Div) * pasta.cell(row = Linha, column = Col + 1).font.size\n",
    "\n",
    "            if mul > 0:\n",
    "                multiples_of_font_size2.append(mul)\n",
    "\n",
    "            pasta.cell(row = Linha, column = Col + 1).alignment = Alignment(wrap_text = wrap_text, vertical = vertical, horizontal = \"center\")\n",
    "\n",
    "    original_height = pasta.row_dimensions[Linha].height\n",
    "    \n",
    "    if original_height is None:\n",
    "        original_height = default_height\n",
    "        \n",
    "    VetorTotal = multiples_of_font_size2\n",
    "\n",
    "    new_height = max(VetorTotal)\n",
    "    \n",
    "    if original_height < new_height:\n",
    "        pasta.row_dimensions[Linha].height = new_height\n",
    "    return\n"
   ]
